{
  "cli": {
    "version": ">= 10.9.0"
  },
  "build": {
    "development": {
  "developmentClient": true,
  "distribution": "internal",
  "node": "20.11.1",
  "android": {
    "buildType": "apk",
    "image": "latest"
  },
  "env": {
    "EXPO_NO_KOTLIN_WARNING": "1",
    "EXPO_USE_SYSTEM_KOTLIN": "true",
    "EXPO_NO_DOTENV": "1"
  }
}
,
    "preview": {
      "distribution": "internal",
      "node": "20.11.1",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "node": "20.11.1",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_KOTLIN_WARNING": "1",
        "EXPO_USE_SYSTEM_KOTLIN": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
