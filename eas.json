{
  "cli": {
    "version": ">= 3.16.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_KOTLIN_WARNING": "1",
        "EXPO_USE_SYSTEM_KOTLIN": "true",
        "EXPO_NO_DOTENV": "true",
        "EAS_DISABLE_NPM_CI": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
